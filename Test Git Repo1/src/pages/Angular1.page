<apex:page standardStylesheets="false" sidebar="false" showHeader="false">
    <html xmlns:ng="http://angularjs.org" ng-app="hello1" lang="en">
        <head>
           <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular.min.js"></script>
        </head>
        <body>     
            <div id="helloApp" >  
                <div ng-controller="controller1">
                <div>
                    <input type="text" ng-model="query" placeholder="Search"/>
                </div>
                <div>
                    You Entered : {{query}}
                </div>
                </div>
            </div>
        </body>
        <script type="text/javascript"> 
            var helloApp = angular.module('helloApp', ['ui.bootstrap','ngCookies']);
            helloApp.controller('controller1', function ($scope) {});
            angular.bootstrap(document.getElementById("helloApp"),['helloApp']);
        </script>
    </html>
</apex:page>